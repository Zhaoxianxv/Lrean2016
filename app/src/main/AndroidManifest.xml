<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxx.lrean.lrean2016">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--引导界面-->
        <activity android:name=".index.IndexActivity" />
        <activity android:name=".sliding.SlidingActivity" />
        <!--索引联系人界面-->
        <activity android:name=".indexableList.IndexableListActivity" />
        <!--扫描二维码界面-->
        <activity android:name=".quick_mark.QuickMarkActivity" />
        <activity android:name=".recycierview.RecycierViewActivity"></activity>
        <!--banner界面-->
        <activity android:name=".banner.BannerActivity"></activity>
    </application>

</manifest>
